{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "eir-frontend",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "eir-frontend": {
      "root": "apps/eir-frontend",
      "sourceRoot": "apps/eir-frontend/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/eir-frontend",
            "index": "apps/eir-frontend/src/index.html",
            "main": "apps/eir-frontend/src/main.tsx",
            "polyfills": "apps/eir-frontend/src/polyfills.ts",
            "tsConfig": "apps/eir-frontend/tsconfig.app.json",
            "assets": [
              "apps/eir-frontend/src/favicon.ico",
              "apps/eir-frontend/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/eir-frontend/src/environments/environment.ts",
                  "with": "apps/eir-frontend/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "eir-frontend:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "eir-frontend:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/eir-frontend/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/eir-frontend"
          ],
          "options": {
            "jestConfig": "apps/eir-frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "eir-frontend-e2e": {
      "root": "apps/eir-frontend-e2e",
      "sourceRoot": "apps/eir-frontend-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/eir-frontend-e2e/cypress.json",
            "tsConfig": "apps/eir-frontend-e2e/tsconfig.e2e.json",
            "devServerTarget": "eir-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "eir-frontend:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/eir-frontend-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "eir-browser": {
      "root": "apps/eir-browser",
      "sourceRoot": "apps/eir-browser/src",
      "projectType": "application",
      "prefix": "eir-browser",
      "targets": {
        "build": {
          "executor": "nx-electron:build",
          "options": {
            "outputPath": "dist/apps/eir-browser",
            "main": "apps/eir-browser/src/main.ts",
            "tsConfig": "apps/eir-browser/tsconfig.app.json",
            "assets": [
              "apps/eir-browser/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/eir-browser/src/environments/environment.ts",
                  "with": "apps/eir-browser/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "nx-electron:execute",
          "options": {
            "buildTarget": "eir-browser:build"
          }
        },
        "package": {
          "executor": "nx-electron:package",
          "options": {
            "name": "eir-browser",
            "frontendProject": "eir-frontend",
            "outputPath": "dist/packages",
            "prepackageOnly": true
          }
        },
        "make": {
          "executor": "nx-electron:make",
          "options": {
            "name": "eir-browser",
            "frontendProject": "eir-frontend",
            "outputPath": "dist/executables"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/eir-browser/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/eir-browser"
          ],
          "options": {
            "jestConfig": "apps/eir-browser/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}